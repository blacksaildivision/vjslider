{
  "name": "vjslider",
  "version": "3.1.2",
  "description": "Super small and responsive vanilla js slider",
  "type": "module",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack serve --mode development",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "NODE_OPTIONS='--experimental-vm-modules' jest --config ./spec/unit-tests/jest.config.js ./spec/unit-tests",
    "test:e2e": "NODE_OPTIONS='--experimental-vm-modules' jest --transform='{}' ./spec/e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blacksaildivision/vjslider.git"
  },
  "keywords": [
    "slider",
    "js",
    "carousel",
    "vjslider",
    "responsive",
    "webpack",
    "es6"
  ],
  "author": "Black Sail Division",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/blacksaildivision/vjslider/issues"
  },
  "homepage": "https://github.com/blacksaildivision/vjslider#readme",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@eslint/js": "^9.17.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^9.17.0",
    "eslint-webpack-plugin": "^4.2.0",
    "globals": "^15.13.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss-loader": "^8.1.1",
    "puppeteer": "^23.10.4",
    "sass": "^1.83.0",
    "sass-loader": "^16.0.4",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0"
  },
  "browserslist": [
    "> 1%"
  ]
}
