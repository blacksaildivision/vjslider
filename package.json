{
  "name": "vjslider",
  "version": "3.1.1",
  "description": "Super small and responsive vanilla js slider",
  "type": "module",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack serve --mode development",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "NODE_OPTIONS='--experimental-vm-modules' jest --config ./spec/unit-tests/jest.config.js ./spec/unit-tests",
    "test:e2e": "NODE_OPTIONS='--experimental-vm-modules' jest --transform='{}' ./spec/e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blacksaildivision/vjslider.git"
  },
  "keywords": [
    "slider",
    "js",
    "carousel",
    "vjslider",
    "responsive",
    "webpack",
    "es6"
  ],
  "author": "Black Sail Division",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/blacksaildivision/vjslider/issues"
  },
  "homepage": "https://github.com/blacksaildivision/vjslider#readme",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.33.0",
    "eslint-webpack-plugin": "^4.0.0",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss-loader": "^7.0.2",
    "puppeteer": "^19.6.3",
    "sass": "^1.58.0",
    "sass-loader": "^13.2.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "browserslist": [
    "> 1%"
  ]
}
