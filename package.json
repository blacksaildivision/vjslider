{
  "name": "vjslider",
  "version": "3.1.2",
  "description": "Super small and responsive vanilla js slider",
  "type": "module",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack serve --mode development",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "NODE_OPTIONS='--experimental-vm-modules' jest --config ./spec/unit-tests/jest.config.js ./spec/unit-tests",
    "test:e2e": "NODE_OPTIONS='--experimental-vm-modules' jest --transform='{}' ./spec/e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blacksaildivision/vjslider.git"
  },
  "keywords": [
    "slider",
    "js",
    "carousel",
    "vjslider",
    "responsive",
    "webpack",
    "es6"
  ],
  "author": "Black Sail Division",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/blacksaildivision/vjslider/issues"
  },
  "homepage": "https://github.com/blacksaildivision/vjslider#readme",
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.10",
    "autoprefixer": "^10.4.15",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.48.0",
    "eslint-webpack-plugin": "^4.0.1",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-loader": "^7.3.3",
    "puppeteer": "^21.1.1",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "browserslist": [
    "> 1%"
  ]
}
